generator client {
  provider = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
}

model Rsvp {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  quantity  Int
  createdAt DateTime @default(now())
}

model Sku {
  id              Int      @id @default(autoincrement())
  nome            String
  descricao       String
  valor           Decimal  @db.Decimal(10, 2)
  link_da_imagem  String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
